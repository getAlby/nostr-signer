{
  "expo": {
    "name": "Alby Nostr Signer",
    "slug": "nostr-signer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "nip46",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.getalby.nostr-signer",
      "usesNonExemptEncryption": false
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.getalby.nostr_signer",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "6aedd85e-7cba-497f-ad8a-043887f92707"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "roland_alby",
    "plugins": ["expo-router"]
  }
}
