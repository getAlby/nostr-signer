{
  "expo": {
    "name": "alby-signer",
    "slug": "alby-signer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "nip46",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "alby-signer",
      "usesNonExemptEncryption": false
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.getalby.albysigner",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "1f250a80-8840-4ee3-a440-ba2c78d17ec8"
      }
    },
    "owner": "kiwiidb",
    "plugins": ["expo-router"]
  }
}
